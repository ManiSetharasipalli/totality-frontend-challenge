datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Booking {
  id            Int      @id @default(autoincrement())
  userId        Int
  propertyId    Int
  startDate     DateTime
  endDate       DateTime
  totalCost     Decimal
  status        String
  paymentMethod String
}
